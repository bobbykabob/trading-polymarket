# Environment variables for API credentials
# Copy this file to .env and add your actual API keys

# Kalshi API credentials
# Get these from: https://kalshi.com/profile/api
KALSHI_API_KEY=your_kalshi_api_key_here
KALSHI_API_SECRET=your_kalshi_api_secret_here

# Polymarket API credentials (if needed)
PRIVATE_KEY=your_polymarket_private_key_here
PROXY_ADDRESS=your_polymarket_proxy_address_here
